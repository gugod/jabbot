#!/usr/bin/env perl
use v5.18;
use FindBin;
use lib "$FindBin::Bin/../lib";

use Jabbot;
use Mojo::Server::Morbo;

use Data::Dump qw(dump);

my $config = Jabbot->config->{twitter};

dump($config);

my $morbo = Mojo::Server::Morbo->new;
$morbo->daemon->listen([ $config->{listen} ]);
$morbo->run("lib/Jabbot/Front/Twitter.pm");
